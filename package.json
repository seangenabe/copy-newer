{
  "name": "copy-newer",
  "version": "1.0.2",
  "description": "Copy newer files only.",
  "main": "./lib/copy.js",
  "bin": {
    "copy-newer": "./bin/copy-newer"
  },
  "scripts": {
    "transpile": "babel src --out-dir lib",
    "watch": "npm run transpile -- --watch",
    "pretest": "npm run transpile",
    "start": "node ./lib/cli.js",
    "prepublish": "npm run transpile"
  },
  "repository": "seangenabe/copy-newer",
  "keywords": [
    "newer",
    "cached",
    "file",
    "fs",
    "filesystem",
    "glob"
  ],
  "author": "Sean Genabe <seangenabe@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seangenabe/copy-newer/issues"
  },
  "homepage": "https://github.com/seangenabe/copy-newer#readme",
  "dependencies": {
    "globby": "^4.0.0",
    "graceful-fs": "^4.1.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-plugin-transform-es2015-destructuring": "^6.5.0",
    "babel-plugin-transform-es2015-parameters": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "delayer": "^1.0.1",
    "mocha": "^2.3.2",
  },
  "engines": {
    "node": ">=4"
  },
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      "transform-es2015-destructuring",
      "transform-es2015-parameters"
    ]
  }
}
